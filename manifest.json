{
    "name": "Auto Duo",
    "version": "0.1.0",
    "description": "Autocomplete Duo two-factor authentication",
    "permissions": [
        "storage",
        "tabs",
        "https://shb.ais.ucla.edu/*",
        "https://api-d594f029.duosecurity.com/*"
    ],
    "browser_action": {
        "default_popup": "popup.html", 
        "default_icon": {
            "16": "images/Icon-16.png",
            "32": "images/Icon-32.png",
            "48": "images/Icon-48.png",
            "128": "images/Icon-128.png"
        }
    },
    "icons": {
        "16": "images/Icon-16.png",
        "32": "images/Icon-32.png",
        "48": "images/Icon-48.png",
        "128": "images/Icon-128.png"
    },
    "manifest_version": 2,
    "browser_specific_settings": {
        "gecko": {
            "id": "auto-duo@paulzzy.github.io"
        }
    }
}